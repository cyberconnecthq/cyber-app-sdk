{
  "name": "@cyberlab/cyber-app-sdk",
  "version": "3.0.0-beta.5",
  "description": "Create a CyberWallet app easily with CyberApp SDK.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "test": "jest",
    "build": "pnpm run test && pnpm run clean && tsup src/index.ts --dts --format esm,cjs --clean",
    "prepack": "pnpm run build",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.9.3",
    "babel-jest": "^29.6.3",
    "jest": "^29.6.3",
    "prettier": "^3.0.2",
    "semantic-release": "^22.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.1.6"
  },
  "keywords": [],
  "author": "CyberConnect",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyberconnecthq/cyber-app-sdk.git"
  },
  "dependencies": {
    "@wagmi/connectors": "^3.1.10",
    "@wagmi/core": "^2.6.5",
    "ts-node": "^10.9.1",
    "viem": "^2.8.9",
    "wagmi": "^2.5.7"
  }
}
